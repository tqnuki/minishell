NAME		= minishell

SRCS    	= minishell.c \
				split.c \
				builtin.c \
				utils.c \

GCC			= gcc -Wall -Wextra -Werror -lreadline
LIBS 		= libft/libft.a
OBJS		= ${SRCS:.c=.o}

$(NAME): ${OBJS}
		@make -C libft
		@$(GCC) -o $(NAME) $(OBJS) $(LIBS)
		@echo Compiled All Sources!

all: $(NAME)

clean:
			@make clean -C libft
			@rm -rf ${OBJS}
			@echo Removed All Object Files!

fclean:	clean
			@make fclean -C libft
			@rm -rf $(NAME)
			@echo Removed Executable!

re:				fclean all

